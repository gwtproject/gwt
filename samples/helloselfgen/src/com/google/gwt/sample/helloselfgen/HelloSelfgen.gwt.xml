<module rename-to="helloselfgen">
	<inherits name="com.google.gwt.user.User" />

	<inherits name="com.google.gwt.sample.helloselfgen.selfgen.Selfgen" />

	<entry-point class="com.google.gwt.sample.helloselfgen.client.HelloSelfgen" />

	<!-- Changing the linker is only necessary if you want to try out SuperDevMode -->
