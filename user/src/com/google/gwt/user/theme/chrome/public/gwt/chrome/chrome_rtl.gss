
body, 
table td, 
select {
  font-family: 'Arial Unicode MS',Arial,sans-serif;
  font-size: small;
}

pre {
  font-family: "courier new",courier;
  font-size: small;
}

body {
  color: black;
  margin: 0;
  border: 0;
  padding: 0;
  background: #fff;
  direction: rtl;
}

a, 
